{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@legacycomponents/*": [
        "./src/legacycomponents/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@type/*": [
        "./src/types/*"
      ],
    }
  }
}
